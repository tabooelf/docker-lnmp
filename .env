#################### MYSQL #####################

MYSQL_VERSION=8.0
MYSQL_DATABASE=default
MYSQL_USER=default
MYSQL_PASSWORD=secret
MYSQL_PORT=3306
MYSQL_ROOT_PASSWORD=root
MYSQL_ENTRYPOINT_INITDB=./mysql/docker-entrypoint-initdb.d

#################### Nginx #####################

NGINX_VERSION=1.15.7-alpine
NGINX_HTTP_HOST_PORT=80
